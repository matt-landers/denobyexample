{
  "compilerOptions": {
    "allowJs": true,
    "esModuleInterop": true,
    "lib": ["esnext"],
    "jsx": "react",
    "module": "esnext",
    "moduleResolution": "node",
    "noEmit": true,
    "plugins": [{ "name": "deno_ls_plugin" }],
    "pretty": true,
    "resolveJsonModule": true,
    "target": "esnext"
  },
  "include": ["./**/*.ts", "routes/react.tsx"]
}
